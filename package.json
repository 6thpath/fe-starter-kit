{
  "name": "fe-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "serve": "serve -s build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "preanalyze": "GENERATE_SOURCEMAP=true react-app-rewired build",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@fsp/design-system": "^0.1.1",
    "customize-cra": "1.0.0",
    "react": "17.0.2",
    "react-app-rewire-postcss": "3.0.2",
    "react-app-rewired": "2.1.8",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "source-map-explorer": "2.5.2",
    "typescript": "4.2.4",
    "web-vitals": "1.1.1",
    "webrtc-adapter": "7.7.1"
  },
  "devDependencies": {
    "@fsp/eslint-config": "^0.2.7",
    "@fsp/prettier": "^1.0.0",
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "eslint": "7.24.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4"
  },
  "prettier": "@fsp/prettier",
  "eslintConfig": {
    "extends": [
      "react-app",
      "@fsp/eslint-config/typescript-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
